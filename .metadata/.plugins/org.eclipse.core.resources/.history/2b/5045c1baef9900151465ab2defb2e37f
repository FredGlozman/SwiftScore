	<?php
	$action = $_POST["action"]
	if($action == "1")
	{
		header('Location: CreateTeam.html') 
	}
	if($action == "2")
	{
		header('Location: CreatePlayer.html')
	}
	if($action == "3")
	{
		header('Location: RemoveTeam.html')
	}
	if($action == "4")
	{
		header('Location: RemovePlayer.html')
	}

	?>