<html>
	<head>
		<title> Authentication</title>
	</head>
	<body>
		<?php
		$array = array(
			"user" => "password",
			);
		$username = $_POST["user"]
		$password = $_POST["password"]
		if($array[0] != $username)
		{
			echo Authentication failed
		}
		else if($array[1] != $password)
		{
			echo Authentication failed
		}
		else
		{
			header('Location: ') //how to get input from first html???
		}
		?>
	</body>
</html>